<%= error_messages_for(@account) %>
<h2>Log in</h2>
  <%= form_tag session_path(@account) do %>
  <p>
    <%= label_tag :email %><br />
    <%= email_field_tag :email %>
  </p>
  <p>
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </p>
  </p><%= submit_tag "Log in" %></p>
<% end %>
